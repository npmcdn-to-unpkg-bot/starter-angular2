{
  "name": "angular2-quickstart",
  "version": "1.0.0",
  "scripts": {
    "start": "concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "scssc": "sh ./scripts/scssc",
    "scssc:w": "sh ./scripts/scsscw",
    "lite": "lite-server",
    "typings": "typings",
    "postinstall": "typings install"
  },
  "license": "ISC",
  "dependencies": {
    "angular2": "2.0.0-beta.15",
    "systemjs": "0.19.26",
    "es6-shim": "0.35.0",
    "es6-class-mixin": "1.0.5",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.2",
    "zone.js": "0.6.10"
  },
  "devDependencies": {
    "concurrently": "2.0.0",
    "lite-server": "2.2.0",
    "node-sass": "3.4.2",
    "nodemon": "1.9.1",
    "typescript": "1.8.10",
    "typings":"0.7.12"
  }
}
